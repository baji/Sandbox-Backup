    <rules>
        <fullName>CA Credit Card</fullName>
        <actions>
            <name>CA_Credit_Card</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <description>Call integration for charging Canada credit card orders</description>
        <formula>Company__c ==&apos;Awana Canada&apos;  &amp;&amp;  ISPICKVAL(Payment_Terms__c, &apos;Credit Card&apos;)  &amp;&amp;  Chargent_Order__r.ChargentOrders__Manual_Charge__c</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Canada default Company</fullName>
        <actions>
            <name>Canada_Order_Company</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Assign Awana Canada as the country for Canada orders</description>
        <formula>Account.Name = &apos;Awana International Canada&apos; || Account.Mailing_Address_Book__c =&apos;4&apos;</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Clear Error Log When Order Closes</fullName>
        <actions>
            <name>Clear_Error_Log_When_Order_Closes</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Order.Status</field>
            <operation>equals</operation>
            <value>Order Invoiced,Credit Issued,Order Cancelled</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Email Credit Card Receipt</fullName>
        <actions>
            <name>EmailcreditcardreceipttocustomerwhenordergoestoJDE</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND 2 AND 3 AND 4 AND 5 AND 6 AND 7 AND 8 AND 9</booleanFilter>
        <criteriaItems>
            <field>Order.Status</field>
            <operation>equals</operation>
            <value>Order Confirmed</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Payment_Terms__c</field>
            <operation>equals</operation>
            <value>Credit Card</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.CreatedById</field>
            <operation>notEqual</operation>
            <value>Nate Kauffman</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.TotalAmount</field>
            <operation>greaterThan</operation>
            <value>USD 0</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Company__c</field>
            <operation>equals</operation>
            <value>Awana</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Opportunity_Source__c</field>
            <operation>notEqual</operation>
            <value>NotTheEnd</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.ExtraEmail__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Has_Donation__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Has_brite__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
