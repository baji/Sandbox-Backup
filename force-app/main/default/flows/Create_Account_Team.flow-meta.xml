        <expression>{!myVariable_current.RecordType.Name} = &quot;US Organization&quot; &amp;&amp;  ISPICKVAL({!myVariable_current.Type} , &quot;Church&quot;) &amp;&amp; ISPICKVAL({!myVariable_current.Registration_Level__c} , &quot;R1&quot;) &amp;&amp; ISCHANGED({!myVariable_current.OwnerId} ) &amp;&amp; NOT(ISNULL({!myVariable_current.Physical_County__c})) &amp;&amp; 
