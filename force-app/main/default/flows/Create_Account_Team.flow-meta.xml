    <description>Create Account Team when Membership Purchased or Account Owner changed.</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>[Account].RecordType.Name = &quot;US Organization&quot;  &amp;&amp;  ISPICKVAL([Account].Type , &quot;Church&quot;)  &amp;&amp;
(ISPICKVAL([Account].Registration_Level__c , &quot;R1&quot;) &amp;&amp; ISCHANGED([Account].Registration_Level__c )  &amp;&amp; ISPICKVAL(PriorValue([Account].Registration_Level__c) , &quot;R0&quot;)  &amp;&amp;
NOT(ISBLANK([Account].Physical_County__c)) &amp;&amp; NOT(ISBLANK([Account].Physical_State__c))
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>{!myVariable_current.RecordType.Name} = &quot;US Organization&quot;  &amp;&amp;  ISPICKVAL({!myVariable_current.Type} , &quot;Church&quot;)  &amp;&amp;
(ISPICKVAL({!myVariable_current.Registration_Level__c} , &quot;R1&quot;) &amp;&amp; ISCHANGED({!myVariable_current.Registration_Level__c} )  &amp;&amp; ISPICKVAL(PriorValue({!myVariable_current.Registration_Level__c}) , &quot;R0&quot;)  &amp;&amp;
NOT(ISBLANK({!myVariable_current.Physical_County__c})) &amp;&amp; NOT(ISBLANK({!myVariable_current.Physical_State__c}))
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>[Account].RecordType.Name = &quot;US Organization&quot; &amp;&amp;  ISPICKVAL([Account].Type , &quot;Church&quot;) &amp;&amp; ISPICKVAL([Account].Registration_Level__c , &quot;R1&quot;) &amp;&amp; ISCHANGED([Account].OwnerId ) &amp;&amp; NOT(ISNULL([Account].Physical_County__c)) &amp;&amp; 
